{
  "name": "promql",
  "rules": {
    "query": {
      "type": "SYMBOL",
      "name": "_query_expression"
    },
    "_query_expression": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_literal_expression"
        },
        {
          "type": "SYMBOL",
          "name": "_timeseries_selector_expression"
        },
        {
          "type": "SYMBOL",
          "name": "_function_expression"
        }
      ]
    },
    "_literal_expression": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "float_literal"
        },
        {
          "type": "SYMBOL",
          "name": "string_literal"
        }
      ]
    },
    "float_literal": {
      "type": "PATTERN",
      "value": "[-+]?([0-9]*\\.?[0-9]+([eE][-+]?[0-9]+)?|0[xX][0-9a-fA-F]+|[nN][aA][nN]|[iI][nN][fF])"
    },
    "string_literal": {
      "type": "SYMBOL",
      "name": "_quoted_string"
    },
    "_timeseries_selector_expression": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "instant_vector_selector"
        },
        {
          "type": "SYMBOL",
          "name": "range_vector_selector"
        }
      ]
    },
    "instant_vector_selector": {
      "type": "SYMBOL",
      "name": "_series_matcher"
    },
    "range_vector_selector": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_series_matcher"
        },
        {
          "type": "SYMBOL",
          "name": "range_selection"
        }
      ]
    },
    "range_selection": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "["
        },
        {
          "type": "SYMBOL",
          "name": "_duration"
        },
        {
          "type": "STRING",
          "value": "]"
        }
      ]
    },
    "_series_matcher": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "metric_name"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "label_selectors"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "label_selectors": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "label_matcher"
                },
                {
                  "type": "REPEAT",
                  "content": {
                    "type": "SEQ",
                    "members": [
                      {
                        "type": "STRING",
                        "value": ","
                      },
                      {
                        "type": "SYMBOL",
                        "name": "label_matcher"
                      }
                    ]
                  }
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    },
    "label_matcher": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "label_name"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "STRING",
              "value": "="
            },
            {
              "type": "STRING",
              "value": "!="
            },
            {
              "type": "STRING",
              "value": "=~"
            },
            {
              "type": "STRING",
              "value": "!~"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "label_value"
        }
      ]
    },
    "metric_name": {
      "type": "PATTERN",
      "value": "[a-zA-Z_:][a-zA-Z0-9_:]*"
    },
    "label_name": {
      "type": "PATTERN",
      "value": "[a-zA-Z_][a-zA-Z0-9_]*"
    },
    "label_value": {
      "type": "SYMBOL",
      "name": "_quoted_string"
    },
    "_function_expression": {
      "type": "SYMBOL",
      "name": "function_call"
    },
    "function_call": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "function_name"
        },
        {
          "type": "SYMBOL",
          "name": "function_args"
        }
      ]
    },
    "function_name": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "rate"
        },
        {
          "type": "STRING",
          "value": "label_join"
        },
        {
          "type": "STRING",
          "value": "histogram_quantile"
        }
      ]
    },
    "function_args": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "("
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "_query_expression"
                },
                {
                  "type": "REPEAT",
                  "content": {
                    "type": "SEQ",
                    "members": [
                      {
                        "type": "STRING",
                        "value": ","
                      },
                      {
                        "type": "SYMBOL",
                        "name": "_query_expression"
                      }
                    ]
                  }
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": ")"
        }
      ]
    },
    "_quoted_string": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_single_quoted_string"
        },
        {
          "type": "SYMBOL",
          "name": "_double_quoted_string"
        }
      ]
    },
    "_double_quoted_string": {
      "type": "PATTERN",
      "value": "\"((\\\\\")|[^\"(\\\\\")])+\""
    },
    "_single_quoted_string": {
      "type": "PATTERN",
      "value": "'((\\\\')|[^'(\\\\')])+'"
    },
    "_duration": {
      "type": "REPEAT1",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "PATTERN",
            "value": "[0-9]+"
          },
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "STRING",
                "value": "ms"
              },
              {
                "type": "STRING",
                "value": "s"
              },
              {
                "type": "STRING",
                "value": "m"
              },
              {
                "type": "STRING",
                "value": "h"
              },
              {
                "type": "STRING",
                "value": "d"
              },
              {
                "type": "STRING",
                "value": "w"
              },
              {
                "type": "STRING",
                "value": "y"
              }
            ]
          }
        ]
      }
    },
    "comment": {
      "type": "TOKEN",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": "#"
              },
              {
                "type": "PATTERN",
                "value": ".*"
              }
            ]
          },
          {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": "//"
              },
              {
                "type": "PATTERN",
                "value": ".*"
              }
            ]
          },
          {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": "/*"
              },
              {
                "type": "PATTERN",
                "value": "[^*]*\\*+([^/*][^*]*\\*+)*"
              },
              {
                "type": "STRING",
                "value": "/"
              }
            ]
          }
        ]
      }
    }
  },
  "extras": [
    {
      "type": "SYMBOL",
      "name": "comment"
    },
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}

